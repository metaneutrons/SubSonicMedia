{
  "metadata": [
    {
      "src": [
        {
          "files": ["SubSonicMedia.csproj"],
          "exclude": ["**/bin/**", "**/obj/**"],
          "src": "SubSonicMedia"
        }
      ],
      "dest": "docfx/api",
      "manifest": "docfx/.manifest"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["*.yml", "index.md"],
        "src": "docfx/api",
        "dest": "api"
      },
      {
        "files": ["index.md", "articles/*.md", "blueprint.md", "toc.yml"],
        "src": "docfx",
        "toc": "toc.yml"
      }
    ],
    "resource": [
      {
        "files": [
          "assets/logo.svg",
          "assets/favicon.svg"
        ],
        "src": "."
      }
    ],
    "overwrite": [],
    "dest": "_site",
    "template": ["default", "docs/templates/SingulinkFX/singulinkfx"],
    "postProcessors": ["ExtractSearchIndex"],
    "globalMetadata": {
      "_appTitle": "EnvoyConfig",
      "_appName": "Documentation",
      "_appFaviconPath": "assets/favicon.svg",
      "_appLogoPath": "assets/logo.svg",
      "_appFooter": "<strong>DocFX + Singulink = ♥</strong>",
      "_copyrightFooter": "© Fabian Schmieder. Licensed under GPL3",
      "_enableSearch": true,
      "_disableSideFilter": false,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_disableBreadcrumb": false
    },
    "noLangKeyword": false
  },
  "api": [
    {
      "src": ["api/**.yml"],
      "dest": "docs/api",
      "manifest": "docs/.manifest"
    }
  ],
  "toc": "docs/toc.yml"
}
